{
  "headers": [
    {
      "source": "/",
      "headers" : [
        {
          "key" : "Clear-Site-Data",
          "value" : "cache, cookies, storage"
        }
      ]
    },
    {
      "source": "/:path*.html",
      "headers" : [
        {
          "key" : "Clear-Site-Data",
          "value" : "cache, cookies, storage"
        }
      ]
    },
    {
      "source": "/:path*/",
      "headers" : [
        {
          "key" : "Clear-Site-Data",
          "value" : "cache, cookies, storage"
        }
      ]
    },
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=1, stale-while-revalidate=59"
        }
      ]
    },
    {
      "source": "/images/",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "s-maxage=500"
        }
      ]
    }
  ]
}
